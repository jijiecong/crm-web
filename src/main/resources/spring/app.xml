		<property name="maxHeight" value="${sharePicConstant.maxHeight}"/>
		<property name="maxWidth" value="${sharePicConstant.maxWidth}"/>
		<property name="height" value="${sharePicConstant.height}"/>
		<property name="width" value="${sharePicConstant.width}"/>
	</bean>

	<context:annotation-config />


</beans>
