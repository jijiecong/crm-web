     <bean id="bossConstant" class="com.meiren.common.constant.BossConstant">
    	<property name="signKey" value="${account.invoke.sign.key}"/>
    	<property name="phpApiRootUrl" value="${account.root.api.url}"/>
     	<property name="secretKey" value="${meiren.common.api.secretkey}"/>
    </bean>
	<bean id="ossConstant" class="com.meiren.common.constant.OssConstant">
		<property name="userId" value="${oss.userId}"/>
		<property name="ossStsUrl" value="${oss.sts.url}"/>
		<property name="ossSecretKey" value="${oss.secretKey}"/>
		<property name="cdnPath" value="${cdnPath}"/>
	</bean>

	<bean id="templatePicConstant" class="com.meiren.common.constant.TemplatePicConstant">
		<property name="qualityRatio" value="${templatePicConstant.qualityRatio}"/>
		<property name="sharpen" value="${templatePicConstant.sharpen}"/>
		<property name="maxSize" value="${templatePicConstant.maxSize}"/>
		<property name="maxHeight" value="${templatePicConstant.maxHeight}"/>
		<property name="maxWidth" value="${templatePicConstant.maxWidth}"/>
		<property name="height" value="${templatePicConstant.height}"/>
		<property name="width" value="${templatePicConstant.width}"/>
	</bean>

	<bean id="templateBannerPicConstant" class="com.meiren.common.constant.TemplateBannerPicConstant">
		<property name="qualityRatio" value="${templateBannerPicConstant.qualityRatio}"/>
		<property name="sharpen" value="${templateBannerPicConstant.sharpen}"/>
		<property name="maxSize" value="${templateBannerPicConstant.maxSize}"/>
		<property name="maxHeight" value="${templateBannerPicConstant.maxHeight}"/>
		<property name="maxWidth" value="${templateBannerPicConstant.maxWidth}"/>
		<property name="height" value="${templateBannerPicConstant.height}"/>
		<property name="width" value="${templateBannerPicConstant.width}"/>
	</bean>

