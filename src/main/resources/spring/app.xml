		<property name="sharpen" value="${missionPicConstant.sharpen}"/>
		<property name="maxSize" value="${missionPicConstant.maxSize}"/>
		<property name="maxHeight" value="${missionPicConstant.maxHeight}"/>
		<property name="maxWidth" value="${missionPicConstant.maxWidth}"/>
		<property name="height" value="${missionPicConstant.height}"/>
		<property name="width" value="${missionPicConstant.width}"/>
	</bean>

	<bean id="missionDetailPicConstant" class="com.meiren.common.constant.MissionDetailPicConstant">
		<property name="qualityRatio" value="${missionDetailPicConstant.qualityRatio}"/>
		<property name="sharpen" value="${missionDetailPicConstant.sharpen}"/>
		<property name="maxSize" value="${missionDetailPicConstant.maxSize}"/>
		<property name="maxHeight" value="${missionDetailPicConstant.maxHeight}"/>
		<property name="maxWidth" value="${missionDetailPicConstant.maxWidth}"/>
